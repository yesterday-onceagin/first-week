{
  "name": "react-dmp-boilerplate",
  "version": "2.0.0",
  "description": "boilerplate for dmp reactJs project",
  "main": "index.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "prod": "node build/dev-server.js --prod true",
    "build": "node build/build.js",
    "build:chart": "node build/build.js --buildMode chart",
    "build:simulator": "node build/build.js --buildMode simulator",
    "build:chartsdk-prod": "node build/build.js --buildMode chartsdk --env prod",
    "build:chartsdk-dev": "node build/build.js --buildMode chartsdk --env dev",
    "build:dll": "cross-env NODE_ENV=production webpack --config build/webpack.dll.conf.js",
    "build:devdll": "cross-env NODE_ENV=development webpack --config build/webpack.dll.conf.js",
    "lint": "eslint --ext .js,.jsx src chartlibs --fix"
  },
  "pre-commit": {
    "run": []
  },
  "repository": {
    "type": "git",
    "url": "git@git.mysoft.com.cn:mic/dmp-web.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "hmr"
  ],
